{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "no-use-before-define": "off",
    "no-unused-vars": "off",
    "no-param-reassign": "off",
    "no-console": "off",
    "no-return-assign": "off"
  }
}
